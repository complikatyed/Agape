<!DOCTYPE html>
<html>
<head>
  <title>Agape</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
  <%= render 'layouts/header' %>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, id: "flash#{name}" %>
  <% end %>
  <%= yield %>

</body>
</html>
